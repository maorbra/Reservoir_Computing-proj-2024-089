{
  "exp": "hyperopt_sin_square_random_search",
  "simulation": false,
  "dataset": "data/sin_square.pickle",
  "hp_max_evals": 10000,
  "hp_method": "random",
  "seed": 3,
  "test_size": 0.5,
  "n_cross_validations": 10,
  "loss_metric": "nmse",
  "inst_per_trial": 3,
  "batch": 32,
  "scope_type": "moku",
  "continue_search": true,
  "duration": 24,
  "calibrate_every": 100,
  "verbosity": 20,
  "hp_space":  {
    "pmax": ["uniform", 0.05,0.7],
    "phi0": ["uniform", 0,3.14],
    "nodes": ["quniform", 10, 50,1],
    "neuron_duration": ["choice", 1],
    "logging_mode": ["choice", "Normal"],
    "logging_rate": ["choice", 488300.0],
    "fir_in_gain": ["choice", 0],
    "fir_rate": ["choice", "976.6kHz"],
    "fir_length": ["quniform", 2, 464, 1],
    "rescale_input": ["choice", false],
    "input_connectivity": ["choice", 1],
    "input_scaling": ["uniform",0.1,0.7],
    "ridge": ["loguniform", 1e-7, 0.1],
    "readout_bias": ["choice", true]
  }
}
